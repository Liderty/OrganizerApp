<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.liber.organizer">

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_main"
        android:label="@string/app_name"
        android:roundIcon="@drawable/ic_main"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".TaskGoalsActivity"></activity>
        <activity android:name=".IconPickerActivity" />
        <activity android:name=".TaskListActivity" />
        <activity android:name=".EditTaskActivity" />
        <activity android:name=".EditCategoryActivity" />
        <activity android:name=".AddCategoryActivity" />
        <activity android:name=".TaskActivity" />
        <activity android:name=".AddTaskActivity" />
        <activity android:name=".AddGoalActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>